{
    "name": "dynasm-vscode",
    "displayName": "Dynasm C Syntax",
    "description": "LuaJIT Dynasm C Syntax (x64)",
    "icon": "icon.png",
    "version": "0.0.2",
    "scripts": {
        "test": ""
    },
    "keywords": [
        "c",
        "luajit",
        "dynasm",
        "syntax",
        "textmate",
        "highlighting",
        "coloring",
        "color"
    ],
    "engines": {
        "vscode": "^1.40.0"
    },
    "publisher": "4984",
    "contributes": {
        "languages": [
            {
                "id": "dasc",
                "aliases": ["dasc", "dynasmc"],
                "extensions": [".dasc"],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "dasc",
                "scopeName": "source.dasc",
                "path": "./syntax/dasc.tmLanguage.json"
            },
            {
                "language": "c",
                "scopeName": "source.cbase",
                "path": "./syntax/cbase.tmLanguage.json"
            }
        ]
    },
    "categories": [
        "Programming Languages"
    ],
    "repository": {
        "type": "git"
    }
}
