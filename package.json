{
  "name": "better-c-syntax",
  "displayName": "Better C Syntax",
  "description": "A placeholder (which will later be the bleeding edge of C syntax)",
  "icon": "icon.png",
  "version": "0.0.2",
  "scripts": {
    "build": "bundle exec ruby main/generator.rb",
    "test": "",
    "vscode_publish": ""
  },
  "keywords": [
    "syntax",
    "textmate",
    "highlighting",
    "coloring",
    "color"
  ],
  "engines": {
    "vscode": "^1.0.0"
  },
  "author": {
    "name": "Jeff Hykin"
  },
  "publisher": "jeff-hykin",
  "contributes": {
    "grammars": [
      {
        "language": "c",
        "scopeName": "source.c",
        "path": "./generated/syntax.tmLanguage.json",
        "embeddedLanguages": {
          "meta.embedded.assembly.c": "asm"
        }
      }
    ]
  },
  "categories": [
    "Programming Languages"
  ],
  "repository": {
    "url": "https://github.com/jeff-hykin/better-c-syntax",
    "type": "git"
  },
  "devDependencies": {
    "chalk": "^2.4.2",
    "convert-svg-to-png": "^0.5.0",
    "dictionary-en-us": "^2.1.1",
    "glob": "^7.1.4",
    "js-yaml": "^3.13.1",
    "json-source-map": "^0.4.0",
    "json-stable-stringify": "^1.0.1",
    "lodash": "^4.17.13",
    "minimatch": "^3.0.4",
    "node-gyp": "^4.0.0",
    "nspell": "^2.1.2",
    "oniguruma": "^7.2.0",
    "prettyjson": "^1.2.1",
    "vscode-textmate": "^4.1.1",
    "yargs": "^13.2.2",
    "textmate-bailout": "^1.1.0"
  },
  "dependencies": {
  }
}