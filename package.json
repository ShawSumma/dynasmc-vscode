{
	"name": "dynasm-vscode",
	"displayName": "DynasmC (arch x64)",
	"description": "LuaJIT Dynasm C Syntax (x64)",
	"icon": "icon.png",
	"version": "0.0.6",
	"keywords": [
		"c",
		"luajit",
		"dynasm",
		"syntax",
		"textmate",
		"highlighting",
		"coloring",
		"color",
		"dasc"
	],
	"engines": {
		"vscode": "^1.85.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ShawSumma/dynasmc-vscode.git"
	},
	"publisher": "4984",
	"contributes": {
		"languages": [
			{
				"id": "dasc",
				"aliases": ["DynasmC", "dasc", "dash"],
				"extensions": [".dasc",".dash"],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "dasc",
				"scopeName": "source.dasc",
				"path": "./syntax/dasc.tmLanguage.json"
			}
		]
	}
}
